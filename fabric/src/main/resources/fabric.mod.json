{
  "schemaVersion": 1,
  "id": "${mod_id}",
  "version": "${mod_version}",

  "name": "${mod_name}",
  "description": "${mod_description}",
  "authors": [
    "${mod_authors}"
  ],
  "contact": {},

  "license": "${mod_license}",
  "icon": "icon.png",

  "environment": "*",
  "entrypoints": {
    "main": [
      "dev.chililisoup.condiments.fabric.CondimentsFabric"
    ],
    "client": [
      "dev.chililisoup.condiments.fabric.CondimentsClientFabric"
    ],
    "jei_mod_plugin": [
      "dev.chililisoup.condiments.compat.JeiCompat"
    ],
    "rei_client": [
      "dev.chililisoup.condiments.compat.ReiCompat"
    ]
  },
  "mixins": [
    "condiments.mixins.json",
    "condiments-common.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=${fabric_loader_version}",
    "minecraft": "=${minecraft_version}"
  }
}
